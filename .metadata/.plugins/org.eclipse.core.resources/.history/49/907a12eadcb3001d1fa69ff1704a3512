package chapter1.F_Operator;

public class Operator {
	public static void main(String[] args) {
		int num = 11;
		
		String re = num == 10 ? "true" : "false";
		System.out.println(re);
		
		// 증감 연산자
		int integer1 = 10;
		++integer1;
		System.out.println(integer1);
		integer1++;
		System.out.println(integer1);
	}
}
